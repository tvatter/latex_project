@Article{Czado2006,
  author   = {Czado, Claudia and Raftery, Adrian E.},
  title    = {{Choosing the link function and accounting for link uncertainty in generalized linear models using Bayes factors}},
  journal  = {Statistical Papers},
  year     = {2006},
