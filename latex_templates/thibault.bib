@Article{voorman2013graph,
  author        = {Voorman, Arend and Shojaie, Ali and Witten, Daniela},
  title         = {{Graph estimation with joint additive models}},
  journal       = {Biometrika},
  year          = {2014},
  volume        = {101},
  number        = {1},
  pages         = {85--101},
  month         = mar,
  issn          = {00063444},
  abstract      = {In recent years, there has been considerable interest in estimating conditional independence graphs in high dimensions. Most previous work assumed that the variables are multivariate Gaussian or that the conditional means of the variables are linearly related. Unfortunately, if these assumptions are violated, the resulting conditional independence estimates can be inaccurate. We propose a semiparametric method, graph estimation with joint additive models, which allows the conditional means of the features to take an arbitrary additive form. We present an efficient algorithm for computation of our estimator, and prove that it is consistent. We extend our method to estimation of directed graphs with known causal ordering. Using simulated data, we show that our method performs better than existing methods when there are nonlinear relationships among the features, and is comparable to methods that assume multivariate normality when the conditional means are linear. We illustrate our method on a cell signalling dataset.},
  archiveprefix = {arXiv},
  arxivid       = {arXiv:1304.4654v2},
  doi           = {10.1093/biomet/ast053},
  eprint        = {arXiv:1304.4654v2},
  keywords      = {Conditional independence,Graphical model,Lasso,Non-Gaussianity,Nonlinearity,Sparse additive model,Sparsity},
  pmid          = {25013234},
  publisher     = {Biometrika Trust},
}

